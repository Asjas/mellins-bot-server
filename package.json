{
  "name": "mellins-bot-server",
  "main": "dist/index.js",
  "author": "A-J Roos <asjas@hey.com>",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "postinstall": "npm run prisma:generate",
    "start": "node dist/index.js",
    "build": "concurrently \"npm run tsc\" \"npm run build:next\"",
    "build:next": "next build",
    "dev": "nodemon",
    "debug:dev": "DEBUG=prisma-redis-middleware nodemon",
    "clean": "rimraf dist .next",
    "test": "jest",
    "test:w": "jest --watchAll",
    "test:inspect": "tap -O -T --node-arg=--inspect-brk",
    "coverage": "npm test -- --coverage-report=lcov",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:pull": "prisma db pull",
    "prisma:studio": "prisma studio",
    "docker:dev:up": "docker-compose -f docker-compose-dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose-dev.yml down -d",
    "docker:dev:nuke": "docker-compose -f docker-compose-dev.yml down -v --rmi all",
    "docker:prod:up": "docker-compose -f docker-compose-prod.yml up -d --build",
    "docker:prod:down": "docker-compose -f docker-compose-prod.yml down -d",
    "tsc": "tsc --project tsconfig.server.json",
    "check-types": "tsc",
    "lint": "eslint --ext .ts .",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .eslintignore \"**/*.+(ts)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build"
  },
  "dependencies": {
    "@headlessui/react": "1.4.1",
    "@heroicons/react": "1.0.5",
    "@prisma/client": "3.3.0",
    "@tailwindcss/forms": "0.3.4",
    "argon2": "0.28.2",
    "date-fns": "2.25.0",
    "dotenv": "10.0.0",
    "fastify": "3.22.1",
    "fastify-autoload": "3.9.0",
    "fastify-cookie": "5.3.1",
    "fastify-cors": "6.0.2",
    "fastify-csrf": "3.1.0",
    "fastify-env": "2.1.1",
    "fastify-etag": "2.1.0",
    "fastify-favicon": "3.1.0",
    "fastify-formbody": "5.1.0",
    "fastify-helmet": "5.3.2",
    "fastify-jwt": "3.2.1",
    "fastify-multipart": "5.0.2",
    "fastify-nextjs": "6.0.0",
    "fastify-nodemailer": "5.0.0",
    "fastify-plugin": "3.0.0",
    "fastify-sensible": "3.1.1",
    "fastify-session": "5.2.1",
    "fastify-static": "4.4.2",
    "fastify-telegraf": "1.0.2",
    "fluent-json-schema": "3.0.1",
    "hyperid": "2.3.1",
    "input": "1.0.1",
    "ioredis": "4.28.0",
    "mjml": "4.10.4",
    "mysql": "2.18.1",
    "next": "12.0.1",
    "pino-pretty": "7.1.0",
    "prisma-redis-middleware": "0.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "telegraf": "4.4.2",
    "telegram": "1.9.6",
    "under-pressure": "5.8.0",
    "undici": "4.8.2"
  },
  "devDependencies": {
    "@tailwindcss/ui": "0.7.2",
    "@types/dotenv": "8.2.0",
    "@types/ioredis": "4.27.8",
    "@types/jest": "27.0.2",
    "@types/mjml": "4.7.0",
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.10",
    "autoprefixer": "10.4.0",
    "concurrently": "6.3.0",
    "eslint-config-asjas": "1.0.0",
    "jest": "27.3.1",
    "nodemon": "2.0.14",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.11",
    "prisma": "3.3.0",
    "rimraf": "3.0.2",
    "tailwindcss": "2.2.17",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  }
}

{
  "name": "node-fastify-typescript-template",
  "main": "dist/index.mjs",
  "author": "A-J Roos <asjas@hey.com>",
  "type": "module",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "start": "node dist/index.mjs",
    "build": "npm-run-all clean build:docker",
    "build:docker": "babel --extensions '.ts' src/ --out-dir dist/ --out-file-extension .mjs",
    "dev": "nodemon",
    "debug:dev": "DEBUG=prisma-redis-middleware nodemon",
    "clean": "rimraf dist",
    "test": "jest",
    "test:w": "jest --watchAll",
    "test:inspect": "tap -O -T --node-arg=--inspect-brk",
    "coverage": "npm test -- --coverage-report=lcov",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "docker:dev:up": "docker-compose -f docker-compose-dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose-dev.yml down -d",
    "docker:dev:nuke": "docker-compose -f docker-compose-dev.yml down -v --rmi all",
    "docker:prod:up": "docker-compose -f docker-compose-prod.yml up -d --build",
    "docker:prod:down": "docker-compose -f docker-compose-prod.yml down -d",
    "check-types": "tsc",
    "lint": "eslint --ext .ts .",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .eslintignore \"**/*.+(ts)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build"
  },
  "dependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/plugin-syntax-top-level-await": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@prisma/client": "2.28.0",
    "babel-plugin-module-extension-resolver": "1.0.0-rc.2",
    "dotenv": "10.0.0",
    "fastify": "3.19.2",
    "fastify-env": "2.1.1",
    "fastify-etag": "2.1.0",
    "fastify-favicon": "3.1.0",
    "fastify-helmet": "5.3.2",
    "fastify-plugin": "3.0.0",
    "fastify-sensible": "3.1.1",
    "fastify-telegraf": "1.0.2",
    "fluent-json-schema": "^3.0.1",
    "input": "1.0.1",
    "ioredis": "4.27.6",
    "mysql": "2.18.1",
    "pino-pretty": "5.1.2",
    "prisma-redis-middleware": "0.4.0",
    "telegraf": "4.4.1",
    "telegram": "1.7.23",
    "under-pressure": "5.7.0",
    "undici": "4.2.2"
  },
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/ioredis": "4.26.6",
    "@types/jest": "26.0.24",
    "@types/node": "16.4.3",
    "eslint-config-asjas": "0.0.25",
    "jest": "27.0.6",
    "nodemon": "2.0.12",
    "npm-run-all": "4.1.5",
    "prisma": "3.1.0",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.4",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  }
}
